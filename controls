  <script type="text/javascript" charset="utf-8">
agGrid.LicenseManager.setLicenseKey("Evaluation_License_Not_For_Production_Valid_Until5_January_2019__MTU0NjY0NjQwMDAwMA==14c2b7c7e69b514c1836f70b9f58c8ff");
// specify the columns
var columnDefs = [
    {headerName: "Category", field: "category", width: 150, rowGroup:true, hide: "true", suppressToolPanel: true},
    {headerName: "Title", field: "title", width: 180},
    {headerName: "Type", field: "type", width: 110},
    {headerName: "Nature", field: "nature", width: 110},
    {headerName: "Automation", field: "automation", width: 110},
    {headerName: "Significance", field: "significance", width: 120},
    {headerName: "Validity", field: "validity", width: 120},
    {headerName: "Status", field: "status", width: 120},
    {headerName: "Associated Risks", field: "associated-risks", width: 120},
    {headerName: "Associated Findings", field: "associated-findings", width: 120},
    {headerName: "Last Tested", field: "last-tested", width: 120}
];

var gridOptions = {
    columnDefs: columnDefs,
    animateRows: true,
    enableRangeSelection: true,
    rowData: null,
    enableSorting:true,
    groupDefaultExpanded: -1,
    enableFilter:true
};

document.addEventListener('DOMContentLoaded', function() {
    var gridDiv = document.querySelector('#myGrid');
    new agGrid.Grid(gridDiv, gridOptions);

    // do http request to get our sample data - not using any framework to keep the example self contained.
    // you will probably use a framework like JQuery, Angular or something else to do your HTTP calls.
    var httpRequest = new XMLHttpRequest();
    httpRequest.open('GET', 'https://api.myjson.com/bins/15pwju');
    httpRequest.send();
    httpRequest.onreadystatechange = function() {
        if (httpRequest.readyState === 4 && httpRequest.status === 200) {
            var httpResult = JSON.parse(httpRequest.responseText);
            gridOptions.api.setRowData(httpResult);
        }
    };
});





function onFilterTextBoxChanged() {
    gridOptions.api.setQuickFilter(document.getElementById('filter-text-box').value);
}

// wait for the document to be loaded, otherwise
// ag-Grid will not find the div in the document.
document.addEventListener("DOMContentLoaded", function() {

    // lookup the container we want the Grid to use
    var eGridDiv = document.querySelector('#myGrid');

    // create the grid passing in the div to use together with the columns & data we want to use
    new agGrid.Grid(eGridDiv, gridOptions1);
});


var acc = document.getElementsByClassName("accordion");
var i;

for (i = 0; i < acc.length; i++) {
  acc[i].addEventListener("click", function() {
    this.classList.toggle("active");
    var panel = this.nextElementSibling;
    if (panel.style.maxHeight){
      panel.style.maxHeight = null;
    } else {
      panel.style.maxHeight = panel.scrollHeight + "px";
    } 
  });
}
  </script>
</body>
</html>
